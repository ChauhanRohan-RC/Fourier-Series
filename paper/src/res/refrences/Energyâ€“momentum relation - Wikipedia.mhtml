#mw-mf-page-left { position: fixed; top: 0px; left: 0px; bottom: 0px; min-width: 275px; visibility: hidden; max-width: 80%; z-index: 2; overflow-y: auto; background-color: rgb(234, 236, 240); transform: translate(-100%, 0px); }

